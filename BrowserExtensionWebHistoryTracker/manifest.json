{
    "name": "LabSecureWeb",
    "version": "0.0.1",
    "description": "LabSecure Web Activity Monitoring",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"popup.html"
    },
    "background": {
        "scripts": [ "background.js" ],
        "persistent": true
    },
    "permissions": ["<all_urls>","activeTab", "tabs"]
}